  "main": "dist/vue-color.ssr.js",
  "browser": "dist/vue-color.esm.js",
  "module": "dist/vue-color.esm.js",
  "unpkg": "dist/vue-color.min.js",
  "keywords": [
    "vue-color",
    "color",
    "vuejs"
  ],
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
